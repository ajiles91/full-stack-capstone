{
    "version": 2,
    "alias": "jiles-marketplace-of-ideas",
    "name": "marketplace-of-ideas",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": "^/favicon.ico$", 
          "dest": "/favicon.ico"
        },
        {
          "src": "^/manifest.json$",
          "dest": "/manifest.json"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ]
  }